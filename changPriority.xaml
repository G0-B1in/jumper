        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:попрыженок"
        mc:Ignorable="d"
        Title="Окно изменения приоритета" Height="250" Width="400" ResizeMode="NoResize" Icon="/imgs/Попрыженок.ico">
    <Grid Background="#43DCFE">
        <TextBlock HorizontalAlignment="Left" Margin="10,116,0,0" TextWrapping="Wrap" Text="Изменить приоритет на" VerticalAlignment="Top"/>
        <TextBox x:Name="PriorityCount" HorizontalAlignment="Left" Height="23" Margin="147,115,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="146"/>
        <Button Content="Изменить" HorizontalAlignment="Left" Margin="298,117,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Image HorizontalAlignment="Left" Height="60" Margin="32,10,0,0" VerticalAlignment="Top" Width="61" Source="imgs/Попрыженок.png"/>

    </Grid>
</Window>
